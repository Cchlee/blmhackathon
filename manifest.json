{
    "name": "Tab for Black Lives Matter - Beta Version",
    "version": "1.2",
    "description": "Discover new Black artists, contribute ad revenue to, and educate yourself on the Black lives matter cause.",
    "manifest_version": 2,
    "chrome_url_overrides" : {
        "newtab": "home.html"
    },
    "icons": {
      "128": "icon.png"
    },
    "permissions": [
        "storage", 
        "bookmarks",
        "tabs",
        "unlimitedStorage"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}
